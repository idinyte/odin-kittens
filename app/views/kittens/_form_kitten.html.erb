<%= form_with model: @kitten, data: {turbo: false} do |f| %>
  <%= f.text_field :name, placeholder: 'name' %><br>
  <%= f.number_field :age, in: 0..25, step: 0.1, placeholder: 'age' %><br>
  <%= f.number_field :cuteness, in: 0..10, step: 1, placeholder: 'cuteness' %><br>
  <%= f.number_field :softness, in: 0..10, step: 1, placeholder: 'softness' %><br>
  <%= f.submit %>
<% end %>